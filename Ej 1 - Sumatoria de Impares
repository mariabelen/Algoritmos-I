/ SUMATORIA.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <conio.h>
#include <iostream>

using namespace std;

int main()
{int s, n, i;
cout << "Ingrese el valor de n: ";
cin>> n;
s=0;
for (i=1; i<=n; i++)
	if(i%2!=0)
		s=s+i;
cout<<"La suma es:  "<<s;
getch();
return 0;
}

